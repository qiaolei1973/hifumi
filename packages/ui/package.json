{
  "name": "@hifumi/ui",
  "version": "0.1.0-3",
  "description": "react ui compoents",
  "main": "./lib/index.js",
  "scripts": {
    "clear-es": "rm -rf ./es",
    "clear-lib": "rm -rf ./lib",
    "clear-dist": "rm -rf ./dist",
    "clear-all": "npm run clear-es && npm run clear-lib && npm run  clear-dist",
    "build-es": "tsc && cross-env NODE_ENV=es node scripts/styleTransform.js",
    "build-lib": "tsc -p ./tsconfig.lib.json && cross-env NODE_ENV=lib node scripts/styleTransform.js",
    "build-all": "npm run build-es && npm run build-lib",
    "prepublish": "npm run clear-all && npm run build-all",
    "publish": "npm publish --tag latest --access=public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiaolei1973/hifumi.git"
  },
  "keywords": [
    "ui",
    "react",
    "component"
  ],
  "files": [
    "lib",
    "es"
  ],
  "author": "qiaolei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiaolei1973/hifumi/issues"
  },
  "homepage": "https://github.com/qiaolei1973/hifumi#readme",
  "dependencies": {
    "antd": "^3.6.3",
    "react": "^16.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "postcss": "^6.0.23",
    "precss": "^3.1.2",
    "rollup": "^0.61.2"
  }
}

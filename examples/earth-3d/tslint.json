{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "interface-name": false,
    "no-empty-interface": false,
    "no-empty": false,
    "ordered-imports": false,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "no-debugger": false,
    "one-variable-per-declaration": false,
    "jsx-boolean-value": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}